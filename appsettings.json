{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "StaticFiles": {
        "ServeUnknownFileTypes": true,
        "DefaultContentType": "application/octet-stream"
    },
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 104857600
        }
    }
}
